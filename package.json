{
  "name": "common-energy",
  "version": "0.1.0",
  "repository": "https://github.com/CommonEnergy/comenergy-customer-portal",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=development next start",
    "dev-old": "NODE_ENV=development next",
    "build": "NODE_ENV=development next build && next export -o /public",
    "lint:staged": "lint-staged"
  },
  "engines": {
    "npm": ">=5.0",
    "node": ">=8.10"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.6.0",
    "@segment/snippet": "^4.4.0",
    "axios": "^0.19.0",
    "babel-plugin-emotion": "^10.0.17",
    "classnames": "^2.2.6",
    "formik": "^1.5.8",
    "google-map-react": "^1.1.4",
    "js-cookie": "^2.2.1",
    "next": "^9.0.5",
    "query-string": "^6.8.3",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-geosuggest": "^2.12.0",
    "react-highlight-words": "^0.16.0",
    "react-number-format": "^4.2.0",
    "react-plaid": "^0.10.0",
    "react-select": "^3.0.4",
    "react-spinners": "^0.6.1",
    "react-stripe-elements": "^5.0.0",
    "recharts": "^1.7.1",
    "zippo": "^1.0.1"
  },
  "devDependencies": {
    "lint-staged": "^9.2.5",
    "pre-commit": "1.2.2",
    "prettier": "^1.18.2"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ]
  },
  "pre-commit": "lint:staged",
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 8,
      "sourceType": "module",
      "ecmaFeatures": {
        "jsx": true,
        "impliedStrict": true,
        "experimentalObjectRestSpread": true
      },
      "allowImportExportEverywhere": true
    },
    "plugins": [
      "react",
      "import"
    ],
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "globals": {
      "VERSION": true,
      "IMAGE_ASSETS_URL": true,
      "VIDEO_ASSETS_URL": true,
      "RAW_ASSETS_URL": true
    },
    "env": {
      "es6": true,
      "browser": true,
      "node": true,
      "jest": true
    },
    "rules": {
      "func-names": [
        "error",
        "as-needed"
      ],
      "no-shadow": "error",
      "import/no-mutable-exports": "error",
      "import/no-anonymous-default-export": [
        "error",
        {
          "allowArray": true,
          "allowArrowFunction": true,
          "allowLiteral": true,
          "allowObject": true
        }
      ],
      "no-extra-semi": 0,
      "react/prop-types": 0,
      "react/react-in-jsx-scope": 0,
      "react/no-unescaped-entities": 0,
      "react/jsx-no-target-blank": 0,
      "react/no-string-refs": 0
    }
  },
  "eslintIgnore": [
    "**/node_modules/**/*.js",
    "**/.next/**/*.js"
  ]
}
